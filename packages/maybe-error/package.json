  "files": [
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "echo 'No build step - using TypeScript source directly'",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src",
    "format": "biome format src --write",
    "test": "echo 'No tests yet'"
  },
  "keywords": [
    "typescript",
    "error-handling",
    "result-pattern",
    "maybe-error",
    "type-safe"
