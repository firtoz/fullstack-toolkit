{
  "name": "@firtoz/maybe-error",
  "version": "1.1.0",
  "description": "Type-safe result handling with MaybeError pattern",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
