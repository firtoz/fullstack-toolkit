  "homepage": "https://github.com/firtoz/router-toolkit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/firtoz/router-toolkit.git",
    "directory": "packages/maybe-error"
  },
  "bugs": {
    "url": "https://github.com/firtoz/router-toolkit/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
